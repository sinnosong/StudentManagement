@model IEnumerable<EnrollmentDateGroupDto>

@{
    ViewData["Title"] = "学生信息统计";
}

<h1>学生信息统计</h1>

<div>
    <table class="table table-striped">
        <thead>
            <tr>
                <th>入学时间</th>
                <th>学生人数</th>
            </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.EnrollmentDate)</td>
                <td>@item.StudentCount</td>
            </tr>
        }
    </table>
</div>